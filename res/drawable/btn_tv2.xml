<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff9d77"  />
            <gradient android:endColor="#00EFFF" android:startColor="#00EEFF" />
            <size android:height="60dp" android:width="100dp" />
            <corners android:radius="35dp" />
            <stroke android:width="5dp" android:color="#90ff9d77" android:dashWidth="1dp" android:dashGap="1dp"/>
            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape>
      
    </item>
    
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff9d77" />
            <gradient android:endColor="#00EFFF" android:startColor="#00EEFF" />
              <size android:height="60dp" android:width="100dp" />
            <corners android:radius="35dp" />
            <stroke android:width="5dp" android:color="#90ff9d77" />
            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape>
    </item>

</selector>